let altura = parseFloat(prompt('Qual sua altura? '))
let genero = prompt('Qual seu genero? M- Masculino, F-Feminino')

if(genero == "F"){
    let pesoIdeal = (62.1*altura) - 44.7
    console.log(`Seu peso idel é de KG${pesoIdeal}`)
} else if(genero == "M"){
    let pesoIdeal = (72.7*altura) - 58 
    console.log(`Seu peso idel é de KG${pesoIdeal}`)
}















